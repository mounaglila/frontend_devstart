{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/angular-website",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*).(js|css|ico|jpg|jpeg|png|svg|webp)", "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
